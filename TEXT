(
  [COUNTRY_NAME] = "Brazil"
  AND
  (
    ([TYPE] = 1 AND [RS] <> "BR")
    OR
    ([TYPE] = 2 AND [RS] = "BR")
  )
  AND
  LEFT(IF IsNull([ISIN]) THEN "" ELSE [ISIN] ENDIF, 2) = "BR"
  AND
  (IF IsNull([SAFEKEEPING_COUNTRY]) THEN "" ELSE [SAFEKEEPING_COUNTRY] ENDIF) <> "Brazil"
  AND
  NOT Contains(UPPER(IF IsNull([DESCRIPTION]) THEN "" ELSE [DESCRIPTION] ENDIF), "MUTUAL FUND")
)
