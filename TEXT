(
    (
        REGEX_Match([SECNUM], "^1.*") OR
        REGEX_Match([SECNUM], "^2.*") OR
        REGEX_Match([SECNUM], "^3.*") OR
        REGEX_Match([SECNUM], "^4.*") OR
        REGEX_Match([SECNUM], "^5.*") OR
        [SECNUM] = "0"
    )
    AND [CT] = "FI"
    AND REGEX_Match([ISIN], "^FI.*")
)
OR
(
    REGEX_Match([SECNUM], "^1.*") OR
    REGEX_Match([SECNUM], "^2.*") OR
    REGEX_Match([SECNUM], "^3.*") OR
    REGEX_Match([SECNUM], "^4.*") OR
    REGEX_Match([SECNUM], "^5.*")
)
